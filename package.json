  "scripts": {
    "lint": "eslint --fix -f unix \"src/**/*.{ts,tsx}\" && prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest --coverage",
    "remove-build": "rimraf dist",
    "build": "tsc && yarn run remove-build && cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts",
    "build-dev": "yarn run build --mode development",
    "build-android": "yarn run build && cap run android",
    "build-android-dev": "yarn run build-dev && cap run android",
    "android-watch": "cap run android -l --external && yarn run watch",
    "build-ios": "yarn run build && cap run ios",
    "build-ios-dev": "yarn run build-dev && cap run ios",
    "watch": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack serve --config webpack.config.ts --mode development",
    "check": "ncu",
    "update": "ncu -u && yarn",
    "npm-check": "npm-check",
    "audit-ci": "audit-ci",
    "postinstall": "ionic config set -g npmClient yarn && husky install && husky add .husky/pre-commit \"yarn run lint\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
